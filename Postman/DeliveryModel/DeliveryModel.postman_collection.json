		{
			"name": "GetDeliveryModel",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var delivery = pm.response.json()[0];",
							"",
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Check data types\", function () {",
							"    pm.expect(delivery.id).to.be.a(\"number\");",
							"    pm.expect(delivery.date).to.be.a(\"string\");",
							"    pm.expect(delivery.quantity).to.be.a(\"number\");",
							"    pm.expect(delivery.productId).to.be.a(\"number\");",
							"});",
							"",
							"pm.test(\"Confirm delivery contains all expected keys\", function () {",
							"    pm.expect(delivery).to.have.all.keys(\"id\",\"date\",\"quantity\",\"productId\");",
							"});",
							"",
							"pm.test(\"Delivery response matches expected JSON schema\", function () {",
							"    var schema = {",
							"        \"properties\": {",
							"            \"id\": { \"type\": \"number\" },",
							"            \"date\": { \"type\": \"string\" },",
							"            \"quantity\": { \"type\": \"number\" },",
							"            \"productId\": { \"type\": \"number\" }",
							"        }",
							"    };",
							"    pm.expect(delivery).to.have.jsonSchema(schema);",
							"});",
							"",
							"pm.test(\"Check a value from the response headers\", () => {",
							"  pm.expect(pm.response.headers.get('Content-Type')).to.eql('application/json; charset=utf-8');",
							"});"
						],
